APP_NAME="DEVO - Dashboard Monitoring Depo Sampah"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=devo
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=pusher
CACHE_DRIVER=file
FILESYSTEM_DRIVER=local
QUEUE_CONNECTION=database
SESSION_DRIVER=file
SESSION_LIFETIME=120

MEMCACHED_HOST=127.0.0.1

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=mailhog
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="noreply@devo.com"
MAIL_FROM_NAME="${APP_NAME}"

# Pusher Configuration for Real-time Updates
PUSHER_APP_ID=your-pusher-app-id
PUSHER_APP_KEY=your-pusher-app-key
PUSHER_APP_SECRET=your-pusher-app-secret
PUSHER_APP_CLUSTER=ap1

# Laravel WebSockets (Alternative to Pusher)
LARAVEL_WEBSOCKETS_PORT=6001

# ESP32 API Configuration
ESP32_API_KEY=devo-esp32-api-key-2024

# File Upload Configuration
MAX_UPLOAD_SIZE=2048
ALLOWED_FILE_TYPES=jpeg,png,jpg

